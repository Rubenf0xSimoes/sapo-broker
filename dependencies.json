                    "modules/nav.menu",
                    "themes/nav.menu"
                ],
                "pagination": [
                    "mixins/nav.pagination",
                    "modules/nav.pagination",
                    "themes/nav.pagination"
                ],
                "pills": [
                    "mixins/nav.pills",
                    "modules/nav.pills",
                    "themes/nav.pills"
                ]
            }
